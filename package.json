    "nuxt-studio": "catalog:nuxt",
    "vue-google-maps-community-fork": "catalog:maps"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:tools",
    "@iconify-json/lucide": "catalog:icons",
    "@nuxt/eslint": "catalog:nuxt",
    "@nuxthub/core": "catalog:nuxt",
    "changelogen": "catalog:tools",
